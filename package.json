{
  "name": "feedrender",
  "type": "module",
  "version": "0.0.1",
  "description": "API to render HTML from RSS feed. Built for podcast feeds, powered by unjs/h3.",
  "author": {
    "name": "Ewilan Rivi√®re",
    "email": "ewilan.riviere@gmail.com",
    "url": "https://github.com/ewilan-riviere"
  },
  "license": "BSD-2-Clause",
  "homepage": "https://github.com/kiwilan/feedrender",
  "main": "index.js",
  "scripts": {
    "start": "tsx ./dist/node.js",
    "start:listhen": "listhen --public=false ./dist/index.js",
    "dev": "tsx --watch ./src/node.ts",
    "dev:listhen": "listhen -w ./src/index.ts",
    "build": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "colorthief": "^2.4.0",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.2.7",
    "h3": "^1.8.0",
    "jsonwebtoken": "^9.0.1",
    "listhen": "^1.4.0",
    "node-cron": "^3.0.2",
    "ofetch": "^1.3.3",
    "sanitize-html": "^2.11.0",
    "sqlite3": "^5.1.6",
    "tsx": "^3.12.7",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.6",
    "@types/sanitize-html": "^2.9.0",
    "@vitest/coverage-v8": "^0.34.3",
    "chalk": "^5.3.0",
    "eslint": "^8.47.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
