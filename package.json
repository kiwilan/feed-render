  "dependencies": {
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.2.7",
    "h3": "^1.8.0",
    "listhen": "^1.4.0",
    "ofetch": "^1.3.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/node": "^20.5.6",
    "@vitest/coverage-v8": "^0.34.3",
    "chalk": "^5.3.0",
    "eslint": "^8.47.0",
    "esno": "^0.17.0",
