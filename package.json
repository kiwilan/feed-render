{
  "name": "feed-renderer",
  "version": "1.0.0",
  "description": "Node.js application to render HTML from RSS feed. Built for Podcasts, powered by h3.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "listhen ./dist/index.js",
    "dev:node": "tsx watch ./src/node.ts",
    "dev": "listhen -w ./src/index.ts",
    "build": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "preview": "node ./dist/index.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.2.7",
    "h3": "^1.8.0",
    "listhen": "^1.3.0",
    "ofetch": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitest/coverage-v8": "^0.34.2",
    "chalk": "^5.3.0",
    "eslint": "^8.47.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  }
}
